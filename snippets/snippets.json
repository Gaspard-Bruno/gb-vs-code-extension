			"	const t = usePolyglot('${1:${TM_DIRECTORY/^.*(\\/|\\\\)([^(\\/|\\\\)]+)$/$2/}}')",
			"",
			"	return (",
			"		<View style={styles.container}>",
			"",
			"		</View>",
			"	)",
			"}",
			"",
			"export default ${1:${TM_DIRECTORY/^.*(\\/|\\\\)([^(\\/|\\\\)]+)$/$2/}}",
			""
		],
